!.idea/externalDependencies.xml
!.idea/jsonSchemas.xml
!.idea/modules.xml
!.idea/vcs.xml
.vscode/*
!.vscode/extensions.json
!.vscode/settings.json
!.vscode/tasks.json
lefthook-local.yml

# Operating system artifacts
*~
.DS_Store
Thumbs.db
