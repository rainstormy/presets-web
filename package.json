{
	"$schema": "https://json.schemastore.org/package.json",
	"private": true,
	"bugs": "https://github.com/spdiswal/starter-web/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/spdiswal/starter-web.git"
	},
	"license": "MIT",
	"author": "Steffen Diswal <11992328+spdiswal@users.noreply.github.com> (https://www.disw.dk)",
	"type": "module",
	"scripts": {
		"build": "tsc --build",
		"setup": "yarn install && simple-git-hooks",
		"validate": "yarn run build"
	},
	"simple-git-hooks": {
		"pre-commit": "yarn run build"
	},
	"devDependencies": {
		"simple-git-hooks": "2.9.0",
		"typescript": "5.3.3"
	},
	"packageManager": "yarn@4.0.2",
	"engines": {
		"node": ">=20.0.0"
	},
	"//": {
		"devDependencies": {
			"simple-git-hooks": "This tool lets the `package.json` file define Git hooks.",
			"typescript": "TypeScript is a statically typed programming language on top of JavaScript."
		}
	}
}
